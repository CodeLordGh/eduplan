{
  "name": "@eduflow/file-service",
  "version": "1.0.0",
  "description": "File management service for EduPlan",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@eduflow/common": "workspace:*",
    "@eduflow/constants": "workspace:*",
    "@eduflow/middleware": "workspace:*",
    "@eduflow/types": "workspace:*",
    "@eduflow/validators": "workspace:*",
    "@eduflow/prisma": "workspace:*",
    "@fastify/multipart": "^7.7.3",
    "cloudinary": "^1.41.0",
    "fastify": "^4.21.0",
    "fp-ts": "^2.16.1",
    "io-ts": "^2.2.20",
    "pino": "^9.6.0",
    "redis": "^4.6.8",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
