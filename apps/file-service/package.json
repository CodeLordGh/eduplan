{
  "name": "@eduflow/file-service",
  "version": "1.0.0",
  "description": "File management service for EduPlan",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@eduflow/common": "workspace:*",
    "@eduflow/constants": "workspace:*",
    "@eduflow/middleware": "workspace:*",
    "@eduflow/prisma": "workspace:*",
    "@eduflow/types": "workspace:*",
    "@fastify/multipart": "^7.7.3",
    "cloudinary": "^1.41.3",
    "fastify": "^5.2.1",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.22",
    "pino": "^9.6.0",
    "redis": "^4.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
