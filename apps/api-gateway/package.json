{
  "name": "@eduflow/api-gateway",
  "version": "1.0.0",
  "description": "EduFlow API Gateway Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.0.4",
    "@fastify/redis": "^7.0.2",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "http-proxy": "^1.18.1",
    "ioredis": "^5.5.0",
    "pino": "^9.6.0",
    "fp-ts": "^2.16.9",
    "@eduflow/common": "workspace:*",
    "@eduflow/constants": "workspace:*",
    "@eduflow/logger": "workspace:*",
    "@eduflow/middleware": "workspace:*",
    "@eduflow/types": "workspace:*",
    "@eduflow/events": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
} 