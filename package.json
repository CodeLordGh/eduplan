{
  "$schema": "http://json.schemastore.org/package",
  "name": "eduflow",
  "version": "1.0.0",
  "description": "A comprehensive educational platform connecting schools, teachers, students, and parents",
  "author": {
    "name": "Kossi Mawuena Baboh",
    "email": "mawuenakossibaboh@gmail.com",
    "url": "https://www.linkedin.com/in/mawuena-kossi-baboh/"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:libs": "turbo run build --filter=./libs/*",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.76",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-boundaries": "^3.4.1",
    "eslint-plugin-folders": "^1.0.4",
    "eslint-plugin-functional": "^6.6.3",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.5.2",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.13.4",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  },
  "workspaces": [
    "libs/*",
    "apps/*"
  ],
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.0.4",
    "@fastify/redis": "^7.0.2",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^5.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "fp-ts": "^2.16.9",
    "http-proxy": "^1.18.1",
    "ioredis": "^5.5.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  }
}